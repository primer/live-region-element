{
  "name": "@primer/live-region-element",
  "version": "0.8.0",
  "type": "module",
  "main": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/esm/index.d.ts",
        "require": "./dist/cjs/index.d.cts"
      },
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.cjs"
      },
      "node": {
        "import": "./dist/esm/node/index.js",
        "require": "./dist/cjs/node/index.cjs"
      },
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/primer/live-region-element.git",
    "directory": "packages/live-region-element"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "script/build",
    "lint:npm": "publint --strict",
    "type-check": "tsc --noEmit",
    "watch": "rollup -c --watch"
  },
  "license": "MIT",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@lit-labs/ssr-dom-shim": "^1.3.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^6.0.2",
    "jsdom": "^25.0.1",
    "publint": "^0.3.9",
    "rimraf": "^6.0.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.7.2"
  },
  "sideEffects": [
    "./src/define.ts",
    "./src/index.ts",
    "./dist/esm/define.js",
    "./dist/esm/index.js",
    "./dist/cjs/define.cjs",
    "./dist/cjs/index.cjs",
    "./dist/esm/node/define.js",
    "./dist/esm/node/index.js",
    "./dist/cjs/node/define.cjs",
    "./dist/cjs/node/index.cjs"
  ]
}
